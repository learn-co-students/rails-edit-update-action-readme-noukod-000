<h1>Edit a Post</h1>

<%= form_tag post_path(@post), method: 'put' do %>

  <br />
  <textarea>Post title:</textarea><%= text_field_tag :title, @post.title %>
  <br />
  <textarea>Post Description</textarea><%= text_area_tag :description, @post.description %>
  <br />
  <%= submit_tag "Submit Post" %>
<% end %>
